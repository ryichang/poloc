<!doctype html> 
<html>
<!-- include head partial form ./views/_head.ejs --> 
<!-- this has all of our style and client-side script tags --> 
<% include ./partials/_header %> 

<body> 
	<div class="container">
		<div class="col-md-2 col-sm-6">
			<h2>Pin</h2>
			<div class="row">
				<div id="saved">
					<% for (var i=images.length -1; i > 0; i--) { %>
						<div class='col-md-6 text-center image-block' 'data-lat=' + '<%= lat[i] %>'  + 'data-lng=' + '<%= lng[i] %>' + 'data-imageIds=' + '<%= imageIds[i] %> '><img class='img-responsive' src=<%= images[i] %>><p><%= names[i] %></p></div>
					<% }  %>
				</div>
			</div>
		</div>
		<script src="//maps.googleapis.com/maps/api/js?v=3.exp&sensor=true"></script>
		<div class="col-md-5 col-sm-6">
			<h2>Map</h2>
			<div id="map"></div>
		</div>

		<div class="col-md-4 col-sm-6">
			<h2>Gallery</h2>
			<div class="row">
		    	<div id="gallery"></div>
		  	</div>
		</div>
	</div>
</body>

<% include ./partials/_footer %>
</html> 

<!-- // PUT THIS IN -->
data-imageIds = <%= imageIds[i] %>